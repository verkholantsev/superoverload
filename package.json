{
    "name": "superoverload",
    "version": "1.2.1",
    "description": "Function overload for JavaScript",
    "main": "build/index.js",
    "scripts": {
        "test": "jest",
        "benchmark": "yarn run build && node ./benchmark",
        "build": "rollup -c && prettier build/index.js --write",
        "format": "eslint --fix . && prettier --write **/*.{md,json,js}",
        "lint": "eslint ./**/*.js",
        "flow": "flow .",
        "pre-commit": "yarn run lint && yarn run flow",
        "pre-push": "yarn test",
        "publish:coverage": "jest --coverage && cat coverage/lcov.info | coveralls"
    },
    "keywords": [
        "function",
        "overload",
        "javascript"
    ],
    "author": "Aleksey Verkholantsev <poshli.pismo@gmail.com> (https://github.com/verkholantsev)",
    "contributors": [
        "escaton (https://github.com/escaton)"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/verkholantsev/superoverload.git"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-flow": "^6.23.0",
        "benchmark": "^2.1.4",
        "captain-git-hook": "^1.1.1",
        "coveralls": "^3.0.0",
        "eslint": "^4.17.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-flowtype": "^2.44.0",
        "eslint-plugin-prettier": "^2.6.0",
        "flow-bin": "^0.66.0",
        "jest": "^22.3.0",
        "lodash": "^4.17.5",
        "prettier": "^1.10.2",
        "rollup": "^0.56.1",
        "rollup-plugin-babel": "^3.0.3",
        "rollup-plugin-commonjs": "^8.3.0",
        "rollup-plugin-node-resolve": "^3.0.3"
    },
    "dependencies": {},
    "engines": {
        "node": ">=6"
    }
}
